package br.dio.java;

public class aula1 {

	public static void main(String[] args) {
	
	int i;
	// int i; variavel duplicada
	int I;
	//int 1a; Variavel inicianco com número
	int _1a;
	int $aq;
	//int I = 10; variavel duplicada
	
	i = 5;
	I = 10;
	_1a = 20;
	$aq = 3;
	
	final int j = 10;
	//j = 15; constante não pode ser alterada
	int asrn24678md;
	//int asrn246 78md; variavel com espaço
	int asrn2$4678_md = 10;
	// int asrn2$46%78_md = 10; variavel com caracter especial

	asrn24678md = 100;
	asrn2$4678_md = 10;
	
	int quantidadeProduto = 50;
	//int QuantidadeProduto; variavel não deve começar com letra maiuscula.
	final int NUMERO_TENTATIVA = 5;
	// final int numeroTentativas = 5; variavel constante não deve ser escrita em camel case.
	int QUANTIDADE_OPCOES = 5; // lowercase é para constantes.
	//int qtdProd; variavel não esta bem definida
	
	System.out.println(i);
	System.out.println(I);
	System.out.println(_1a);
	System.out.println($aq);
	
	System.out.println(j);
	System.out.println(asrn24678md);
	System.out.println(asrn2$4678_md);
	
	System.out.println(quantidadeProduto);
	System.out.println(NUMERO_TENTATIVA);
	System.out.println(QUANTIDADE_OPCOES);
	
	}

}
